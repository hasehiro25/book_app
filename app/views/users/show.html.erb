<h1>個人ページ</h1>
<dl>
  <dt>email</dt>
  <dd><%= @user.email %></dd>
  <dt>郵便番号</dt>
  <dd><%= @user.complement_blank_text(@user.postcode) %></dd>
  <dt>住所</dt>
  <dd><%= @user.complement_blank_text(@user.address) %></dd>
  <dt>プロフィール</dt>
  <dd><%= br_with_escape(@user.complement_blank_text(@user.profile)) %></dd>
</dl>
